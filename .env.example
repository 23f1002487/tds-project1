# Environment Variables for Student Task Processor
# Copy this file to .env and fill in your actual values

# Required: Authentication secret for API access
secret=your_secret_key_here

# Required: GitHub Personal Access Token with repo permissions
github_token=your_github_pat_here

# Required: AIPIPE API token for AI-powered code generation
AIPIPE_TOKEN=your_aipipe_token_here

# Optional: AIPIPE service URL (defaults to openrouter if not set)
AIPIPE_URL=https://aipipe.org/openrouter/v1

# Optional: Logging configuration
LOG_LEVEL=INFO
LOG_FILE=/tmp/task_log.txt

# Legacy: Direct OpenAI API support (fallback if AIPIPE not available)
# OPENAI_API_KEY=your_openai_key_here
# OPENAI_URL=https://api.openai.com/v1